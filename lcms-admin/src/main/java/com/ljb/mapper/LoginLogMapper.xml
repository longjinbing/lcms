    <update id="updateByIdSelective" parameterType="com.ljb.entity.LoginLog">
        update sys_login_log
        <set>
            <if test="status != null">
                `status` = #{status},
            </if>
            <if test="ip != null">
                `ip` = #{ip},
            </if>
            <if test="createId != null">
                `create_id` = #{createId},
            </if>
            <if test="createTime != null">
                `create_time` = #{createTime},
            </if>
            <if test="updateId != null">
                `update_id` = #{updateId},
            </if>
            <if test="updateTime != null">
                `update_time` = #{updateTime},
            </if>
            <if test="device != null">
                `device` = #{device},
            </if>
            <if test="isDelete != null">
                `is_delete` = #{isDelete},
            </if>
            <if test="remark != null">
                `remark` = #{remark},
            </if>
            <if test="result != null">
                `result` = #{result}
            </if>
        </set>
        where id = #{id}
    </update>
    <insert id="saveBatch" useGeneratedKeys="true" keyProperty="id" parameterType="java.util.List">
        INSERT INTO sys_login_log
        (
        `id` ,
        `status` ,
        `ip` ,
        `create_id` ,
        `create_time` ,
        `update_id` ,
        `update_time` ,
        `device` ,
        `is_delete` ,
        `remark` ,
        `result` )
        values
        <foreach item="item" index="index" collection="list"
                 separator=",">
            (
            #{item.id},
            #{item.status},
            #{item.ip},
            #{item.createId},
            #{item.createTime},
            #{item.updateId},
            #{item.updateTime},
            #{item.device},
            #{item.isDelete},
            #{item.remark},
            #{item.result} )
        </foreach>
    </insert>

    <update id="updateBatch" parameterType="java.util.List">>
        <foreach item="item" index="index" collection="list" separator=";">
            update sys_login_log
            <set>
                <if test="item.id != null">`id` =
                    #{item.id} ,
                </if>
                <if test="item.status != null">`status` =
                    #{item.status} ,
                </if>
                <if test="item.ip != null">`ip` =
                    #{item.ip} ,
                </if>
                <if test="item.createId != null">`create_id` =
                    #{item.createId} ,
                </if>
                <if test="item.createTime != null">`create_time` =
                    #{item.createTime} ,
                </if>
                <if test="item.updateId != null">`update_id` =
                    #{item.updateId} ,
                </if>
                <if test="item.updateTime != null">`update_time` =
                    #{item.updateTime} ,
                </if>
                <if test="item.device != null">`device` =
                    #{item.device} ,
                </if>
                <if test="item.isDelete != null">`is_delete` =
                    #{item.isDelete} ,
                </if>
                <if test="item.remark != null">`remark` =
                    #{item.remark} ,
                </if>
                <if test="item.result != null">`result` =
                    #{item.result}
                </if>
            </set>
            where `id` = #{item.id}
        </foreach>
    </update>

    <select id="selectMapById" resultMap="DetailsMap">
        SELECT
                t.`id` ,            t.`status` ,            t.`ip` ,            t.`create_id` ,            t.`create_time` ,            t.`update_id` ,            t.`update_time` ,            t.`device` ,            t.`is_delete` ,            t.`remark` ,            t.`result`                     ,s.`username` as createName,
        s2.`username` as updateName
        FROM sys_login_log t left join sys_user s on s.`id`= t.`create_id` LEFT JOIN sys_user s2 ON s2.`id`=t.`update_id`
                 WHERE
                 t.`is_delete`=1 and
                  t.`id`=#{id}
    </select>

    <select id="selectMapList" parameterType="map" resultMap="listResultMap">
        SELECT
        t.`id` , t.`status` , t.`ip` , t.`create_id` , t.`create_time` , t.`update_id` , t.`update_time` , t.`device` ,
        t.`is_delete` , t.`remark` , t.`result` ,s.`username` as updateName
        FROM sys_login_log t left join sys_user s on s.`id`= t.`update_id`
        <where>
            <if test="field != null and search != null">
                t.`${field}` LIKE CONCAT('%', #{search}, '%')
            </if>
            <if test="stime != null and etime != null">
                and t.`create_time` BETWEEN #{stime} AND #{etime}
            </if>
            <if test=" offset != null ">
                and t.`is_delete`=1
            </if>
        </where>
        <choose>
            <when test="sort != null and order != null">
                ORDER BY ${sort} ${order}
            </when>
            <otherwise>
                ORDER BY id DESC
            </otherwise>
        </choose>
        <if test="offset != null and limit != null">
            LIMIT ${offset}, ${limit}
        </if>
    </select>

    <select id="selectTotal" parameterType="map" resultType="long">
        SELECT COUNT(*) FROM sys_login_log
        <where>
            <if test="field != null and search != null">
                #{field} LIKE CONCAT('%', #{search}, '%')
            </if>
            <if test=" offset != null ">
                and `is_delete`=1
            </if>
        </where>
    </select>

